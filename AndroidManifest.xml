<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.finproj"
      android:versionCode="1"
      android:versionName="0.3">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.CAMERA"></uses-permission>

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@android:style/Theme.NoTitleBar" >
        <activity android:name=".FinProj"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="EditPage" android:screenOrientation="portrait"></activity>
        <activity android:name="EditPhoto"></activity>
        <activity android:name="RotatePhoto"></activity>
        <activity android:name="ImageRotate"></activity>
        <activity android:name="ListPage"></activity>
        <activity android:name="OnePhoto"></activity>

    </application>
</manifest>